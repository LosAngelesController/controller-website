{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "~/*": ["./public/*"]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/tooltipsPlot/tooltips.bruh",
    "src/__tests__/pages/pafr22.test.tsx.off",
    "src/pages/landings/2022-employee-residence-analysis/CDBar2.jsx",
    "src/pages/landings/2022-employee-residence-analysis/FiftyKPieOut.jsx",
    "src/pages/landings/2022-employee-residence-analysis/FiftyKPieIn.jsx",
    "src/pages/landings/2022-employee-residence-analysis/HundredKIn.jsx",
    "src/pages/landings/2022-employee-residence-analysis/HundredKOut.jsx",
    "src/pages/landings/2022-employee-residence-analysis/SummaryPie2.jsx",
    "src/pages/landings/2022-employee-residence-analysis/SummaryPie1.jsx"
  ],

  "exclude": ["node_modules", "src/__tests__", "src/lib"],
  "moduleResolution": ["node_modules", ".next", "node"]
}
