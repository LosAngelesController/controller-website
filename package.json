    "@observablehq/plot": "^0.6.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/d3": "^7.4.0",
    "@types/google-translate-api": "^2.3.2",
    "byte-size": "^8.1.0",
    "calendar": "^0.1.1",
    "clsx": "^1.2.1",
    "d3": "^7.8.2",
    "d3-svg-legend": "^2.25.6",
    "edit-json-file": "^1.7.0",
    "google-translate-api": "^2.3.0",
    "jquery": "^3.6.3",
    "jsdom": "^21.0.0",
    "legacystyles": "^1.0.0",
    "month": "^2.0.1",
