  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/byte-size": "^8.1.0",
    "@types/calendar": "^0.1.2",
    "@types/google.maps": "^3.51.0",
    "@types/jquery": "^3.5.16",
